{
  "title": "RED - Example Service",
  "panels": [
    {
      "type": "timeseries",
      "title": "Request Duration P99",
      "targets": [
        {"expr": "histogram_quantile(0.99, sum(rate(http_server_request_duration_seconds_bucket[5m])) by (le))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Rate",
      "targets": [
        {"expr": "sum(rate(http_server_requests_total[1m]))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5xx)",
      "targets": [
        {"expr": "sum(rate(http_server_requests_total{code=~\"5..\"}[1m]))"}
      ]
    }
  ],
  "schemaVersion": 38,
  "version": 1
}

